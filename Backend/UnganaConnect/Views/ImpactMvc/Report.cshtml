@model dynamic

@{
    ViewData["Title"] = "Impact Report";
}

<h1>Impact Report</h1>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Summary Statistics</h5>
        <ul class="list-group list-group-flush">
            <li class="list-group-item">Total Users: @Model.Users</li>
            <li class="list-group-item">Total Courses: @Model.Courses</li>
            <li class="list-group-item">Total Enrollments: @Model.Enrollments</li>
            <li class="list-group-item">Total Resources: @Model.Resources</li>
            <li class="list-group-item">Consultancy Requests: @Model.ConsultancyRequests</li>
        </ul>
    </div>
</div>

<div class="mt-3">
    <button onclick="window.print()" class="btn btn-primary">Print Report</button>
    <a href="/ImpactMvc/Overview" class="btn btn-secondary">Back to Overview</a>
</div>
